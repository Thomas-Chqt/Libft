{
	"C_Cpp.default.includePath": [
		"includes"
	],
	"files.exclude": {
		"*.a": true,
		"build/": true,
		"libft.h": true,
		"Makefile": true,
		"product/": true
	},
	"files.associations": {
		"libft.h": "c",
		"stdio.h": "c",
		"__tuple": "c",
		"array": "c",
		"libft_internal.h": "c",
		"memory_leak_detector.h": "c"
	},
	"autoSnippet.snippets": [
		{
			"pattern": "**/*.c",
			"snippet": "New source file (Libft)"
		},
		{
			"pattern": "**/*.h",
			"snippet": "New header file"
		}
	],
}
